                            +Environment.NewLine
                            + "strFileName_Format_Import :" + Dts.Variables["strFileName_Format_Import"].Value
                            + Environment.NewLine
                            + "strFileName_Format_Export :" + Dts.Variables["strFileName_Format_Export"].Value
                            + Environment.NewLine
                            + "strFromEmail :" + Dts.Variables["strFromEmail"].Value
                            +Environment.NewLine
