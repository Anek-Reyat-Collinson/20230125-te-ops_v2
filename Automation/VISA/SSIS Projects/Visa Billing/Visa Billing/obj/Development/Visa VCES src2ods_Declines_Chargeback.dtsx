    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{F4FBE137-55C5-4C9D-80AC-20B424316906}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;;WITH offerData&#xA;AS (SELECT CAST(ord.outlet_id AS VARCHAR(10)) AS outlet_id,&#xA;           country.code,&#xA;           country.code_desc,&#xA;           ord.track_seq,&#xA;           ord.consumer_no,&#xA;           ord.offer_category,&#xA;           ord.offer_type&#xA;    FROM PPass.dbo.offer_redemption_details AS ord WITH (NOLOCK)&#xA;        INNER JOIN PPass.dbo.airport AS a WITH (NOLOCK)&#xA;            ON a.iso_airport_code = ord.airport_iso_code&#xA;        INNER JOIN PPass.dbo.code country2 WITH (NOLOCK)&#xA;            ON country2.alpha_1 = a.iso_country_code&#xA;               AND country2.codetype = 'CTRY2'&#xA;               AND country2.code NOT IN ( 'CLI', 'ENG', 'IOM', 'NIR', 'SCO', 'WAL', 'ETM', 'CDI', 'SVG' )&#xA;        INNER JOIN PPass.dbo.code country WITH (NOLOCK)&#xA;            ON country2.code = country.code&#xA;               AND country.codetype = 'CTRY'),&#xA;     loungeData&#xA;AS (SELECT l.lounge_code,&#xA;           l.country_code,&#xA;           ct.code_desc&#xA;    FROM PPass.dbo.lounge l WITH (NOLOCK)&#xA;        INNER JOIN PPass.dbo.code ct WITH (NOLOCK)&#xA;            ON l.country_code = ct.code&#xA;               AND ct.codetype = 'CTRY')&#xA;SELECT CASE&#xA;           WHEN s.activity_code = 'NEWB2C'&#xA;                AND ss.subscription_level_id = 1 THEN&#xA;               'Standard'&#xA;           WHEN s.activity_code = 'NEWB2C'&#xA;                AND ss.subscription_level_id = 2 THEN&#xA;               'Standard Plus'&#xA;           WHEN s.activity_code = 'NEWB2C'&#xA;                AND ss.subscription_level_id = 3 THEN&#xA;               'Prestige'&#xA;           WHEN s.activity_code IN ( 'NEWCORPG', 'NEWWHCORPG' ) THEN&#xA;               'Corporate'&#xA;           WHEN s.activity_code IN ( 'NEWWHSL', 'NEWWHSLITE' ) THEN&#xA;               'Wholesale'&#xA;           WHEN s.activity_code IN ( 'NEWAXMP', 'NEWAXAPP', 'NEWAXAPG' ) THEN&#xA;               'Amex'&#xA;           WHEN s.activity_code = 'NEWAFNTY' THEN&#xA;               'Affinity'&#xA;           WHEN s.activity_code = 'NEWWHSLBC' THEN&#xA;               'Bank Card Wholesale'&#xA;           ELSE&#xA;               'Unallocated'&#xA;       END AS deal_type,&#xA;       c.external_identifier,&#xA;       t.track_seq,&#xA;&#x9;   al.consumer_no,&#xA;&#x9;   al.ccard_number&#x9;AS credit_card_number ,&#xA;&#x9;   al.ccard_expiry&#x9;AS credit_card_expiry_date ,&#xA;       c.service_centre,&#xA;       al.account_line_seq as account_line_sequence,&#xA;       al.account_line_status,&#xA;       alStat.code_desc as acccount_line_description,&#xA;       al.account_line_type,&#xA;       al.activity_code,&#xA;       al.batch_no,&#xA;       al.purchase_no,&#xA;       al.glac_code,&#xA;       al.amount,&#xA;       (al.amount * ex.rate) AS us_amount,&#xA;       al.currency,&#xA;       ex.rate,&#xA;       al.ccard_type as cc_card_type,&#xA;       al.charge_back_code,&#xA;       al.posted_date,&#xA;       al.transaction_id,&#xA;       s.source_code,&#xA;       p.paid_to_date,&#xA;       p.subs_type AS member_type,&#xA;       s.source_desc,&#xA;       s.source_media AS source_type,&#xA;       s.owner_product AS Product,&#xA;       s.market_code,&#xA;       ct2.code_desc AS member_status,&#xA;       gl.glac_desc as glac_description,&#xA;       c.country_of_residence,&#xA;       t.track_from AS lounge_code,&#xA;       CASE&#xA;           WHEN t.visit_type_id = 0 THEN&#xA;               ld.country_code&#xA;           ELSE&#xA;               od.code&#xA;       END AS country_code,&#xA;       CASE&#xA;           WHEN t.visit_type_id = 0 THEN&#xA;               ld.code_desc&#xA;           ELSE&#xA;               od.code_desc&#xA;       END AS country_of_visit,&#xA;       t.track_start AS visit_date,&#xA;       1 AS track_mbr,&#xA;       t.track_guests,&#xA;       CASE&#xA;           WHEN t.track_start &lt; paid_to_date THEN&#xA;               'Y'&#xA;           ELSE&#xA;               'N'&#xA;       END AS recharge,&#xA;       CASE&#xA;           WHEN al.amendment_date &gt; paid_to_date THEN&#xA;               'Y'&#xA;           ELSE&#xA;               'N'&#xA;       END AS process_date_GT_PTD,&#xA;       CASE&#xA;           WHEN al.amendment_date &gt;= al.ccard_expiry THEN&#xA;               'Y'&#xA;           ELSE&#xA;               'N'&#xA;       END AS process_date_GT_expiry_date,&#xA;       t.external_ref as voucher_ref,&#xA;       CASE&#xA;           WHEN t.visit_type_id = 0 THEN&#xA;               'Lounge'&#xA;           ELSE&#xA;               od.offer_type&#xA;       END AS experience_type,&#xA;&#xA;       CASE&#xA;           WHEN t.visit_type_id = 0 THEN&#xA;               'Lounge'&#xA;           ELSE&#xA;               od.offer_category&#xA;       END AS brand_category,&#xA;       al.created_by  as accountline_created_by,&#xA;       al.creation_date as accountline_creation_date,&#xA;       al.amended_by as accountline_amended_by,&#xA;       al.amendment_date as accountline_amendment_date,&#xA;&#x9;   CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, al.[creation_date]), 0) AS DATE)      AS &#x9;report_month&#xA;FROM PPass.dbo.account_line al WITH (NOLOCK)&#xA;    INNER JOIN PPass.dbo.tracking t WITH (NOLOCK)&#xA;        ON al.consumer_no = t.consumer_no&#xA;           AND al.track_seq = t.track_seq&#xA;    INNER JOIN PPass.dbo.exchange_rate ex WITH (NOLOCK)&#xA;        ON al.amendment_date &gt;= ex.effective_date&#xA;           AND al.amendment_date &lt; ex.expiry_date&#xA;           AND al.currency = ex.input_currency&#xA;           AND ex.output_currency = 'US'&#xA;    INNER JOIN PPass.dbo.consumer c WITH (NOLOCK)&#xA;        ON al.consumer_no = c.consumer_no&#xA;    INNER JOIN PPass.dbo.purchase p WITH (NOLOCK)&#xA;        ON al.purchase_no = p.purchase_no&#xA;    INNER JOIN PPass.dbo.purchase_subscription_link psl WITH (NOLOCK)&#xA;        ON p.purchase_no = psl.purchase_no&#xA;    INNER JOIN PPass.dbo.source_subscription ss WITH (NOLOCK)&#xA;        ON psl.source_subscription_id = ss.source_subscription_id&#xA;    INNER JOIN PPass.dbo.source_deal_config sdc WITH (NOLOCK)&#xA;        ON ss.source_deal_config_id = sdc.source_deal_config_id&#xA;    INNER JOIN PPass.dbo.source s WITH (NOLOCK)&#xA;        ON sdc.source_code = s.source_code&#xA;    INNER JOIN PPass.dbo.glac gl WITH (NOLOCK)&#xA;        ON al.glac_code = gl.glac_code&#xA;    LEFT JOIN offerData AS od&#xA;        ON t.track_seq = od.track_seq&#xA;           AND t.consumer_no = od.consumer_no&#xA;    LEFT JOIN loungeData AS ld &#xA;        ON t.track_from = ld.lounge_code&#xA;    INNER JOIN PPass.dbo.code ct2 WITH (NOLOCK)&#xA;        ON p.subs_status = ct2.code&#xA;           AND ct2.codetype = 'MSTAT'&#xA;    INNER JOIN PPass.dbo.code alStat WITH (NOLOCK)&#xA;        ON alStat.code = al.account_line_status&#xA;           AND alStat.codetype = 'ACTLNSTAT'&#xA;    INNER JOIN PPass.dbo.code mop WITH (NOLOCK)&#xA;        ON mop.codetype = 'MOP'&#xA;           AND mop.code = al.ccard_type&#xA;WHERE 1 = 1&#xA;      AND al.creation_date &gt;= &quot;+&quot;'&quot;+(DT_WSTR, 4) year( @[User::dtStartDate]) +  right(&quot;00&quot; + (DT_WSTR, 2) month( @[User::dtStartDate] ),2)  + &#xA;&#x9;right(&quot;00&quot; + (DT_WSTR, 2) day( @[User::dtStartDate] ),2) +&quot;'&quot; + &quot;AND al.creation_date &lt;  DATEADD(d,1,'&quot;+ (DT_WSTR, 4) year( @[User::dtEndDate]) +  right(&quot;00&quot; + (DT_WSTR, 2) month( @[User::dtEndDate] ),2)&#xA;+ right(&quot;00&quot; + (DT_WSTR, 2) day( @[User::dtEndDate] ),2)+&quot;'&quot;+&quot;)&quot;&#xA;+&quot; AND al.track_seq &gt; 0 AND al.account_line_status IN ('D0','D9','C') AND s.source_code in (&quot;+ @[$Package::strSourcecodes] + &quot;)&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="strQuery">
      <DTS:VariableValue
        DTS:DataType="8">;WITH offerData
AS (SELECT CAST(ord.outlet_id AS VARCHAR(10)) AS outlet_id,
           country.code,
           country.code_desc,
           ord.track_seq,
           ord.consumer_no,
           ord.offer_category,
           ord.offer_type
    FROM PPass.dbo.offer_redemption_details AS ord WITH (NOLOCK)
        INNER JOIN PPass.dbo.airport AS a WITH (NOLOCK)
            ON a.iso_airport_code = ord.airport_iso_code
        INNER JOIN PPass.dbo.code country2 WITH (NOLOCK)
            ON country2.alpha_1 = a.iso_country_code
               AND country2.codetype = 'CTRY2'
               AND country2.code NOT IN ( 'CLI', 'ENG', 'IOM', 'NIR', 'SCO', 'WAL', 'ETM', 'CDI', 'SVG' )
        INNER JOIN PPass.dbo.code country WITH (NOLOCK)
            ON country2.code = country.code
               AND country.codetype = 'CTRY'),
     loungeData
AS (SELECT l.lounge_code,
           l.country_code,
           ct.code_desc
    FROM PPass.dbo.lounge l WITH (NOLOCK)
        INNER JOIN PPass.dbo.code ct WITH (NOLOCK)
            ON l.country_code = ct.code
               AND ct.codetype = 'CTRY')
SELECT CASE
           WHEN s.activity_code = 'NEWB2C'
                AND ss.subscription_level_id = 1 THEN
               'Standard'
           WHEN s.activity_code = 'NEWB2C'
                AND ss.subscription_level_id = 2 THEN
               'Standard Plus'
           WHEN s.activity_code = 'NEWB2C'
                AND ss.subscription_level_id = 3 THEN
               'Prestige'
           WHEN s.activity_code IN ( 'NEWCORPG', 'NEWWHCORPG' ) THEN
               'Corporate'
           WHEN s.activity_code IN ( 'NEWWHSL', 'NEWWHSLITE' ) THEN
               'Wholesale'
           WHEN s.activity_code IN ( 'NEWAXMP', 'NEWAXAPP', 'NEWAXAPG' ) THEN
               'Amex'
           WHEN s.activity_code = 'NEWAFNTY' THEN
               'Affinity'
           WHEN s.activity_code = 'NEWWHSLBC' THEN
               'Bank Card Wholesale'
           ELSE
               'Unallocated'
       END AS deal_type,
       c.external_identifier,
       t.track_seq,
	   al.consumer_no,
	   al.ccard_number	AS credit_card_number ,
	   al.ccard_expiry	AS credit_card_expiry_date ,
       c.service_centre,
       al.account_line_seq as account_line_sequence,
       al.account_line_status,
       alStat.code_desc as acccount_line_description,
       al.account_line_type,
       al.activity_code,
       al.batch_no,
       al.purchase_no,
       al.glac_code,
       al.amount,
       (al.amount * ex.rate) AS us_amount,
       al.currency,
       ex.rate,
       al.ccard_type as cc_card_type,
       al.charge_back_code,
       al.posted_date,
       al.transaction_id,
       s.source_code,
       p.paid_to_date,
       p.subs_type AS member_type,
       s.source_desc,
       s.source_media AS source_type,
       s.owner_product AS Product,
       s.market_code,
       ct2.code_desc AS member_status,
       gl.glac_desc as glac_description,
       c.country_of_residence,
       t.track_from AS lounge_code,
       CASE
           WHEN t.visit_type_id = 0 THEN
               ld.country_code
           ELSE
               od.code
       END AS country_code,
       CASE
           WHEN t.visit_type_id = 0 THEN
               ld.code_desc
           ELSE
               od.code_desc
       END AS country_of_visit,
       t.track_start AS visit_date,
       1 AS track_mbr,
       t.track_guests,
       CASE
           WHEN t.track_start &lt; paid_to_date THEN
               'Y'
           ELSE
               'N'
       END AS recharge,
       CASE
           WHEN al.amendment_date &gt; paid_to_date THEN
               'Y'
           ELSE
               'N'
       END AS process_date_GT_PTD,
       CASE
           WHEN al.amendment_date &gt;= al.ccard_expiry THEN
               'Y'
           ELSE
               'N'
       END AS process_date_GT_expiry_date,
       t.external_ref as voucher_ref,
       CASE
           WHEN t.visit_type_id = 0 THEN
               'Lounge'
           ELSE
               od.offer_type
       END AS experience_type,

       CASE
           WHEN t.visit_type_id = 0 THEN
               'Lounge'
           ELSE
               od.offer_category
       END AS brand_category,
       al.created_by  as accountline_created_by,
       al.creation_date as accountline_creation_date,
       al.amended_by as accountline_amended_by,
       al.amendment_date as accountline_amendment_date,
	   CAST(DATEADD(MONTH, DATEDIFF(MONTH, 0, al.[creation_date]), 0) AS DATE)      AS 	report_month
FROM PPass.dbo.account_line al WITH (NOLOCK)
    INNER JOIN PPass.dbo.tracking t WITH (NOLOCK)
        ON al.consumer_no = t.consumer_no
           AND al.track_seq = t.track_seq
    INNER JOIN PPass.dbo.exchange_rate ex WITH (NOLOCK)
        ON al.amendment_date &gt;= ex.effective_date
           AND al.amendment_date &lt; ex.expiry_date
           AND al.currency = ex.input_currency
           AND ex.output_currency = 'US'
    INNER JOIN PPass.dbo.consumer c WITH (NOLOCK)
        ON al.consumer_no = c.consumer_no
    INNER JOIN PPass.dbo.purchase p WITH (NOLOCK)
        ON al.purchase_no = p.purchase_no
    INNER JOIN PPass.dbo.purchase_subscription_link psl WITH (NOLOCK)
        ON p.purchase_no = psl.purchase_no
    INNER JOIN PPass.dbo.source_subscription ss WITH (NOLOCK)
        ON psl.source_subscription_id = ss.source_subscription_id
    INNER JOIN PPass.dbo.source_deal_config sdc WITH (NOLOCK)
        ON ss.source_deal_config_id = sdc.source_deal_config_id
    INNER JOIN PPass.dbo.source s WITH (NOLOCK)
        ON sdc.source_code = s.source_code
    INNER JOIN PPass.dbo.glac gl WITH (NOLOCK)
        ON al.glac_code = gl.glac_code
    LEFT JOIN offerData AS od
        ON t.track_seq = od.track_seq
           AND t.consumer_no = od.consumer_no
    LEFT JOIN loungeData AS ld 
        ON t.track_from = ld.lounge_code
    INNER JOIN PPass.dbo.code ct2 WITH (NOLOCK)
        ON p.subs_status = ct2.code
           AND ct2.codetype = 'MSTAT'
    INNER JOIN PPass.dbo.code alStat WITH (NOLOCK)
        ON alStat.code = al.account_line_status
           AND alStat.codetype = 'ACTLNSTAT'
    INNER JOIN PPass.dbo.code mop WITH (NOLOCK)
        ON mop.codetype = 'MOP'
           AND mop.code = al.ccard_type
WHERE 1 = 1
      AND al.creation_date &gt;= '20230101'AND al.creation_date &lt;  DATEADD(d,1,'20230131') AND al.track_seq &gt; 0 AND al.account_line_status IN ('D0','D9','C') AND s.source_code in ('LKVSVISINFUKR18','LKVSVISPLTUKR18','LKVSVISSIGUKR18','LKVSVCEINFUKR19','LKVSVCEPLTUKR19','LKVSVCESIGUKR19','LKVSALFPLTUKR18','LKVSALFINFUKR18','LKVSRAIVARUKR19','LKVSBELINFBLR19','LKVSBELPLTBLR19','LKVSBPSINFBLR18','LKVSMTBINFRUS19','LKVSPRIINFBLR18','LKVSRAIINFSRB19','LKVSRAIPLTSRB19','LKVSSBSINFKAZ19','LKVSINFPRICE16','LKVSMENPLTQA18','LKVSPLTCEM16','LKVSSIGENHKW17','LKVSSOHINFOM18','LKVSSOHSIGOM18','LKVSSPCEGRLS18','LKVSFNBINFZAF18','LKVSIBINFSA2016','LKVSSSAINFZAF17','LKVSSSASIG17','LKVSSSASIGZA18','LKVSINVPLTZAF19','LKVSGAZVINRUS20','LKVSNBKPLTKWT20','LKVSOSCVARUKR20','LKVSFNBINFOMN20')</DTS:VariableValue>
